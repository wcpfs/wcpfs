#!/usr/bin/env bash

root_dir=`dirname $0`/..

server=scheduler
repo_url=`git config remote.origin.url`
remote_deploy_url=monitoringdev@$server:git/`basename $repo_url`

echo "Deploying to $remote_deploy_url"
if [[ ! -n $(git status --porcelain) ]]; then
  git push $remote_deploy_url master
else
  git status
fi
